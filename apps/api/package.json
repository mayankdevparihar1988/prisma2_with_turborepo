{
  "name": "@ibs-lotto/api",
  "version": "1.0.7",
  "description": "Node.js TypeScript Graphql API v1",
  "author": "Nazar Kulyk <nazar.kulyk@ibsolution.de>",
  "repository": "git@github.com:ibsprv/ibslotto.git",
  "type": "module",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "clean:build": "rimraf dist",
    "clean:build:cache": "rimraf build .buildcache",
    "clean": "npm run -s clean:build && npm run -s clean:build:cache",
    "build": "npm run clean && tsc",
    "watch": "npm run clean && tsc -w",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --fix --ext .ts src/",
    "start": "node -r source-map-support/register --max_old_space_size=260 dist/app.js",
    "dev": "nodemon --trace-warnings -r source-map-support/register build/app.js",
    "generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev --name first-migration --skip-seed",
    "db:seed": "node --loader ts-node/esm prisma/seed.ts",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
    "@prisma/client": "^4.10.0",
    "@schamane/serial-exec": "^2.0.2",
    "compression": "^1.7.4",
    "concurrently": "^7.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.0",
    "passport": "^0.6.0",
    "passport-forward-auth": "^1.1.1",
    "source-map-support": "^0.5.21",
    "undici": "^5.10.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/graphql-type-json": "^0.3.2",
    "@types/helmet": "0.0.48",
    "@types/lodash": "^4.14.185",
    "@types/lodash-es": "^4.17.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.13.0",
    "@types/passport": "^1.0.11",
    "@types/styled-components": "^5.1.26",
    "eslint": "8.23.1",
    "eslint-config-custom": "*",
    "nodemon": "^2.0.20",
    "prettier-eslint": "^15.0.1",
    "prisma": "^4.10.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig": "*",
    "typescript": "^4.9.5"
  },
  "nodemonConfig": {
    "ignore": [
      "src/tests/*",
      "docs/*"
    ],
    "params": "--max_old_space_size=200 -r source-map-support/register",
    "delay": "2500"
  },
  "engines": {
    "node": "^16 || ^18",
    "npm": "^8"
  }
}